  "scripts": {
    "build": "./node_modules/.bin/gulp build && ./node_modules/.bin/prettier-eslint --write \"dist/**/*.{js,d.ts}\" || echo \"Failed to pretty all built files.\"",
    "do-publish": "npm run build && npm run lint && ./node_modules/.bin/gulp publish",
    "dry-publish": "npm run build && npm run lint && ./node_modules/.bin/gulp publishDry",
    "format": "./node_modules/.bin/prettier-eslint --write \"{{src,test,demo,tasks}/**/*.{ts,js,jsm,css,scss},./*.{ts,js,jsm,json,md}}\"",
    "format:check": "./node_modules/.bin/prettier-eslint --list-different \"{{src,test,demo,tasks}/**/*.{ts,js,jsm,css,scss},./*.{ts,js,jsm,json,md}}\"",
    "lint": "./node_modules/.bin/gulp lint"
  },
